{"hierarchy":{"paths":[["doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive","doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive\/EXPPredictive"]]},"metadata":{"modules":[{"name":"EXPPredictive"}],"title":"checkIfEligibleForSurvey","fragments":[{"kind":"text","text":"+ "},{"text":"checkIfEligibleForSurvey","kind":"identifier"}],"externalID":"c:objc(cs)EXPPredictive(cm)checkIfEligibleForSurvey","navigatorTitle":[{"text":"checkIfEligibleForSurvey","kind":"identifier"}],"roleHeading":"Type Method","role":"symbol","symbolKind":"method"},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive\/EXPPredictive\/checkIfEligibleForSurvey"},"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"text":"Checks to see if the user is currently eligible to be invited to a survey.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/exppredictive\/exppredictive\/checkifeligibleforsurvey"]}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"+ ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"kind":"identifier","text":"checkIfEligibleForSurvey"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"kind":"content","content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"text":"In order to be eligible, a number of conditions must be fulfilled:","type":"text"}],"type":"paragraph"},{"type":"orderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The library is in an enabled state. If the user has previously declined an invite","type":"text"},{"type":"text","text":" "},{"text":"or completed a survey, the library will be in a disabled state. Otherwise, the library is","type":"text"},{"type":"text","text":" "},{"type":"text","text":"in an enabled state."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"The user has met the loyalty threshold defined in your exp_configuration.json file.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"The user is in the sampling pool. If the previous two conditions have been met, the library"},{"text":" ","type":"text"},{"text":"makes an HTTP call to our servers to determine if the user is in the sampling pool. If s\/he is,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the invite will be presented. If not, the invite will not be shown. Disable this remote check with"},{"type":"text","text":" "},{"identifier":"setSkipPoolingCheck:","isActive":true,"type":"reference"}],"type":"paragraph"}]}]}]}],"sections":[],"references":{"doc://com.Verint.EXPPredictive/documentation/EXPPredictive":{"url":"\/documentation\/exppredictive","identifier":"doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive","kind":"symbol","title":"EXPPredictive","type":"topic","abstract":[],"role":"collection"},"setSkipPoolingCheck:":{"identifier":"setSkipPoolingCheck:","type":"link","titleInlineContent":[{"type":"text","text":"setSkipPoolingCheck:"}],"url":"setSkipPoolingCheck:","title":"setSkipPoolingCheck:"},"doc://com.Verint.EXPPredictive/documentation/EXPPredictive/EXPPredictive/checkIfEligibleForSurvey":{"url":"\/documentation\/exppredictive\/exppredictive\/checkifeligibleforsurvey","identifier":"doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive\/EXPPredictive\/checkIfEligibleForSurvey","role":"symbol","abstract":[{"type":"text","text":"Checks to see if the user is currently eligible to be invited to a survey."}],"fragments":[{"kind":"text","text":"+ "},{"text":"checkIfEligibleForSurvey","kind":"identifier"}],"title":"checkIfEligibleForSurvey","kind":"symbol","type":"topic","navigatorTitle":[{"text":"checkIfEligibleForSurvey","kind":"identifier"}]},"doc://com.Verint.EXPPredictive/documentation/EXPPredictive/EXPPredictive":{"url":"\/documentation\/exppredictive\/exppredictive","identifier":"doc:\/\/com.Verint.EXPPredictive\/documentation\/EXPPredictive\/EXPPredictive","type":"topic","title":"EXPPredictive","kind":"symbol","abstract":[{"text":"The EXPPredictive class provides the main interface to the Verint EXPPredictive module. You must add a","type":"text"},{"type":"text","text":" "},{"code":"exp_configuration.json","type":"codeVoice"},{"text":" file to your project that defines the behavior of the Trigger module (e.g. defines the","type":"text"},{"type":"text","text":" "},{"text":"triggering criteria for inviting a user to participate in a survey).","type":"text"}],"fragments":[{"text":"EXPPredictive","kind":"identifier"}],"role":"symbol"}}}